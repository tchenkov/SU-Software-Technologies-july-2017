{{#if user}}
<div class="container body-content span=8 offset=2">
    <div class="well">
    <form class="form-horizontal" method="post" action="/user/register">
    <fieldset>
    <legend>User details</legend>
    <div class="form-group">
    <label class="col-sm-4 control-label" for="inputEmail">Email</label>
    <div class="col-sm-4 ">
    <input disabled="disabled" type="text" class="form-control" id="inputEmail" placeholder="Email" title="email" required value={{user.email}}>
</div>
</div>
    <div class="form-group">
    <label class="col-sm-4 control-label" for="inputFullName">Full Name</label>
    <div class="col-sm-4 ">
    <input disabled="disabled" type="text" class="form-control" id="inputFullName" placeholder="Full Name" required title="fullName" value={{user.fullName}}>
</div>
</div>
</fieldset>
</form>
</div>
</div>
{{/if}}
